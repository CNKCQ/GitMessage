{
  "name": "gitci",
  "version": "1.0.0",
  "description": "一个 git commit tool",
  "scripts": {
    "start": "sh gitMessage.sh",
    "commit": "git add . && ./node_modules/commitizen/bin/git-cz"
  },
  "repository": {
    "type": "git",
    "url": "null"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cz-customizable": "^6.2.0",
    "husky": "^4.0.0-beta.1"
  },
  "dependencies": {}
}
